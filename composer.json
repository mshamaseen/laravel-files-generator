{
    "name": "shamaseen/laravel-files-generator",
    "description": "Generate any kind of File/s from stubs with only a single command, literally, ANY TYPE.",
    "tags" : [
        "stubs", "templates", "generator", "file generator", "generate files"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "shamaseen",
            "email": "m.shamaseen@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/console": ">5.7",
        "illuminate/support": ">5.7"
    },
    "autoload": {
        "psr-4": {
            "Shamaseen\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JohnDoe\\BlogPackage\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shamaseen\\Generator\\GeneratorServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0"
    }
}
